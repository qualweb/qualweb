{
  "name": "@qualweb/counter",
  "version": "0.2.3",
  "description": "Counts elements of a webpage by role and by tag",
  "main": "dist/counter.bundle.js",
  "files": [
    "dist/*"
  ],
  "types": "prebuild/index.d.ts",
  "scripts": {
    "tsc": "tsc",
    "test": "mocha test/counter.spec.ts",
    "lint": "eslint .",
    "format": "prettier src/**/*.ts --write",
    "prebuild": "npm run format && rimraf prebuild && tsc --build tsconfig.prod.json",
    "build": "npm run prebuild && rimraf dist && webpack",
    "prepublishOnly": "npm run build"
  },
  "homepage": "https://github.com/qualweb/counter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qualweb/counter.git"
  },
  "bugs": {
    "url": "https://github.com/qualweb/counter/issues"
  },
  "keywords": [
    "qualweb",
    "accessibility",
    "a11y",
    "counter",
    "html-roles",
    "html-tags"
  ],
  "author": "Pedro Santos",
  "license": "ISC",
  "devDependencies": {
    "@qualweb/core": "^0.7.43",
    "@qualweb/qw-page": "0.2.21",
    "@qualweb/util": "0.5.41",
    "@tsconfig/recommended": "^1.0.3",
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "^14.14.37",
    "chai": "4.5.0",
    "esm": "^3.2.25",
    "mocha": "^10.2.0",
    "prettier": "^3.1.1",
    "puppeteer": "^22.12.1",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^4.6.0"
  }
}
