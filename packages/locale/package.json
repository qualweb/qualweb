{
  "name": "@qualweb/locale",
  "version": "0.2.1",
  "description": "Locales for @qualweb/core evaluation reports.",
  "files": [
    "dist/*"
  ],
  "exports": {
    ".": {
      "require": "./dist/locale.bundle.js",
      "types": "./prebuild/index.d.ts"
    }
  },
  "scripts": {
    "tsc": "tsc",
    "test": "mocha",
    "lint": "eslint .",
    "format": "prettier src/**/*.ts --write",
    "clean": "rimraf dist",
    "build": "tsc --build tsconfig.prod.json && webpack",
    "prepublishOnly": "npm run build"
  },
  "homepage": "https://github.com/qualweb/qualweb/blob/main/packages/locale",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qualweb/qualweb.git",
    "directory": "packages/locale"
  },
  "bugs": {
    "url": "https://github.com/qualweb/locale/issues"
  },
  "keywords": [
    "qualweb",
    "accessibility",
    "a11y",
    "locale"
  ],
  "author": "Jo√£o Vicente",
  "license": "ISC",
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.3",
    "@types/chai": "^4.3.11",
    "@types/lodash.clonedeep": "^4.5.9",
    "@types/mocha": "^10.0.6",
    "@types/node": "^16.10.1",
    "chai": "4.5.0",
    "mocha": "^10.2.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "lodash.clonedeep": "^4.5.0"
  }
}
