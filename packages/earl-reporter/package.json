{
  "name": "@qualweb/earl-reporter",
  "version": "0.4.0",
  "description": "Qualweb earl reporter",
  "main": "dist/index.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "tsc": "tsc",
    "test": "mocha --require esm",
    "prebuild": "rimraf dist",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "build": "npm run format && npm run lint && npm run prebuild && tsc --build",
    "prepare": "npm run build"
  },
  "keywords": [
    "earl",
    "json-ld",
    "a11y",
    "normalized",
    "report",
    "accessibility"
  ],
  "homepage": "https://github.com/qualweb/earl-reporter#readme",
  "bugs": {
    "url": "https://github.com/qualweb/earl-reporter/issues",
    "email": "qualweb@fc.ul.pt"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qualweb/earl-reporter.git"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "author": "Jo√£o Vicente",
  "license": "ISC",
  "devDependencies": {
    "@qualweb/types": "^0.4.63",
    "@tsconfig/recommended": "^1.0.1",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/node": "^14.14.35",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "chai": "^4.3.4",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-sonarjs": "^0.6.0",
    "esm": "^3.2.25",
    "mocha": "^8.3.2",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "typedoc": "^0.20.33",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "lodash.clonedeep": "^4.5.0"
  }
}
