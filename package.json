    "Antonio Estriga <antonio.estriga@gmail.com>",
    "Jo√£o Afonso Vicente <joao.vicente@edu.ulisboa.pt>",
    "Johannes L. Borresen <jlb@inqludeit.dk>"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/qualweb/qualweb/issues"
  },
  "homepage": "https://github.com/qualweb/qualweb#readme",
  "workspaces": [
    "packages/qw-element",
    "packages/qw-page",
    "packages/util",
    "packages/locale",
    "packages/crawler",
    "packages/core",
    "packages/act-rules",
    "packages/best-practices",
    "packages/wcag-techniques",
    "packages/counter",
    "packages/earl-reporter",
    "packages/cli",
    "packages/cui-checks"
